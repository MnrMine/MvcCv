@using Mvc_Cv.Models.Entity
@model List<TblAdmin>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}




<br />
<h2>Admin  İşlemleri</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Kullanıcı Adı</th>

        <th>Düzenle</th>
        <th>Kaldır</th>

        @foreach (var x in Model)
        {
<tr>
            <td>@x.ID</td>
            <td>@x.KullaniciAdi</td>
          
            <td><a href="/Admin/AdminDuzenle/@x.ID" class="btn btn-info">Düzenle</a></td>
            <td><a href="/Admin/AdminSil/@x.ID" class="btn btn-success">Sil</a></td>


        </tr>
    }
        </tr>
    </table>
    <a href="/Admin/AdminEkle/" class="btn btn-primary">Yeni Kulanıcı Ekle</a>



