@using Mvc_Cv.Models.Entity
@model List<TblEgitimlerim>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<br />
<h2>Egitim Listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>AltBaşlık</th>
        <th>AltBaşlık 2</th>
        <th>GNO</th>
        <th>Tarih</th>
        <th>Sil</th>
        <th>Düzenle</th>
        @foreach (var x in Model)
        {
    <tr>
        <td>@x.ID</td>
        <td>@x.Baslik</td>
        <td>@x.AltBaslik1</td>
        <td>@x.AltBaslik2</td>
        <td>@x.GNO</td>
        <td>@x.Tarih</td>
        <td>
            <a class="btn btn-danger btn-sm" href="/Egitim/EgitimSil/@x.ID">
                <i class="fas fa-trash"></i>
                Sil
            </a>
        </td>
        <td>
            <a class="btn btn-info btn-sm" href="/Egitim/EgitimDüzenle/@x.ID">
                <i class="fas fa-pencil-alt"></i>
                Düzenle
            </a>
        </td>



    </tr>
    }
        </tr>
    </tr>
    </table>
    <a href="/Egitim/EgitimEkle/" class="btn btn-primary">Yeni Egitim Bilgisi Ekle</a>



